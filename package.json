{
  "name": "linkme",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "db:migration:generate": "./node_modules/sequelize-cli/lib/sequelize migration:generate --config ./config/database.js",
    "db:migrate": "./node_modules/sequelize-cli/lib/sequelize db:migrate --config ./config/database.js",
    "db:test:migrate": "NODE_ENV=test npm run db:migrate -- --debug",
    "db:all:migrate": "npm run db:migrate; npm run db:test:migrate",
    "db:migrate:undo": "./node_modules/sequelize-cli/lib/sequelize db:migrate:undo --config ./config/database.js",
    "db:test:migrate:undo": "NODE_ENV=test npm run db:migrate:undo",
    "db:all:migrate:undo": "npm run db:migrate:undo;npm run db:test:migrate:undo",
    "start:dev": "./node_modules/nodemon/bin/nodemon.js --inspect=0.0.0.0:54549 ./bin/www",
    "start": "./node_modules/nodemon/bin/nodemon.js ./bin/www",
    "watch": "NODE_ENV=test node -r ./tests/load-nock-disable-http.js ./node_modules/mocha/bin/_mocha tests/* --recursive --watch --timeout 5000",
    "test": "NODE_ENV=test node -r ./tests/load-nock-disable-http.js ./node_modules/mocha/bin/_mocha tests/* --recursive --exit --timeout 5000",
    "test:unit": "NODE_ENV=test mocha tests/unit/* --recursive  --exit",
    "lint": "node_modules/eslint/bin/eslint.js .",
    "console": "node --experimental-repl-await repl-init",
    "auto:lint:fix": "eslint --fix --ext .js,.jsx ."
  },
  "dependencies": {
    "aws-sdk": "~2.395.0",
    "babel-eslint": "^10.1.0",
    "body-parser": "~1.18.3",
    "chai-datetime": "^1.7.0",
    "chai-http": "^4.3.0",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "deepmerge": "^4.2.2",
    "ejs": "~2.6.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "javascript-state-machine": "^3.1.0",
    "lodash": "^4.17.19",
    "mersenne": "0.0.4",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "mysql2": "^1.7.0",
    "nodemon": "^1.19.1",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "request-promise": "^4.2.5",
    "sequelize": "4.44.4",
    "sequelize-cli": "^5.5.0",
    "slug": "^2.1.1",
    "tunnel-ssh": "^4.1.4",
    "uuid": "^7.0.3",
    "validate.js": "^0.13.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "rewire": "^5.0.0",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0"
  }
}
